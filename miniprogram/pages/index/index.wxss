  height: 138rpx;
}
.page-box {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.page-title {
  width: 60vw;
  margin: 6vh auto 32rpx;
  text-align: center;
  font-size: 128rpx;
  font-weight: 400;
  line-height: 144rpx;
  position: relative;
}
.card-box {
  width: 70%;
  border-radius: 32rpx;
  min-height: 300rpx;
  border: 1px solid #FA5845;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  font-weight: 500;
  color: #FA5845;
  line-height: 64rpx;
  padding-top: 32rpx;
  box-sizing: border-box;
}
.card-box-title {
  position: absolute;
  line-height: 60rpx;
  width: 70%;
  overflow: hidden;
  text-align: center;
  text-overflow: ellipsis;
  color: #fff;
  background: #FA5845;
  border-radius: 8rpx;
  margin: 0 auto;
  top: -30rpx;
  left: 15%;
}
.resume-text {
  text-align: center;
  font-size: 52rpx;
  font-weight: 400;
  margin-bottom: 5vh;
}
.btn-box {
  width: 70vw;
  margin: 0 auto;
  margin:24rpx auto 12rpx;
  justify-content: space-evenly;
  position: fixed;
  top: calc( 800rpx + (100vh - 1044rpx)/2 - 80rpx );
  left: 15vw;
}
